<div class='last'>
  <h2>Login</h2>
  <%= form_tag sessions_path do %>
  <ul>
    <li>
      <%= label_tag 'Username' %>
      <%= text_field_tag :username %>
    </li>
    <li>
      <%= label_tag 'Password' %>
      <%= password_field_tag :password  %>
    </li>
    <li>
      <%= submit_tag %>
    </li>
  </ul>
  <% end %>
</div>