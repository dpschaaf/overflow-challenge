<div class='last'>
  <h2>User Sign Up</h2>
  <%= form_for @user do |f| %>
  <ul>
    <li><%= f.label :username %>
      <%= f.text_field :username %>
    </li>
    <li><%= f.label :password, 'Password' %>
      <%= f.password_field :password %>
    </li>
    <li>
      <%= f.submit %>
    </li>
    <% end %>
  </ul>
</div>