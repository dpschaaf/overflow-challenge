<h1>Welcome to DBC Overflow</h1>
<div class = 'container'>
  <header>
    <span>
      <%= link_to "Sign Up", new_user_path, remote: true %>
    </span>
    <span>
      <%= link_to "Log In", new_sessions_path, remote: true %>
    </span>
    <span>
      <%= link_to "Show Questions", questions_path, remote: true %>
    </span>
    <% if current_user %>
    <span>
      <%= link_to "Add Question", new_question_path, remote: true%>
    </span>
    <% end %>
  </header>
  <%= render 'questions/questions' %>
</div>

